
<% content_for :caboose_css do %>
  <% @page.linked_resources_map[:css].each do |r| %>
    <!--link type='text/css' rel='stylesheet' href="<%= r %>" /-->
    <%= stylesheet_link_tag r %>
  <% end %>
<% end %>

<% content_for :caboose_js do %>
  <% @page.linked_resources_map[:js].each do |r| %>
    <!--script type='text/javascript' src="<%= r %>" ></script-->
    <%= javascript_include_tag r %>
  <% end %>
<% end %>

<% content_for :page_title   do %><%= raw @page.title   %><% end %>
<% content_for :page_content do %><% 
  if @page.title != 'Access Denied'
    @page.blocks.each do |b|  
      bt = b.block_type
      if bt.nil?
        bt = BlockType.where(:name => 'heading').first 
        b.block_type_id = bt.id
        b.save
      end
      if bt.use_render_function && bt.render_function
        %><%= raw b.render_from_function(params[:empty_text]) %><%        
      else        
        %><%= render :partial => "caboose/blocks/#{bt.name}", :locals => { :block => b, :empty_text => "", :editing => false } %><%
      end
    end
  end
%><% end %>
